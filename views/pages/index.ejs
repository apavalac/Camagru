<%- include('../includes/header.ejs'); %>

<main>
	<div class="jumbotron">
        <h1>This is great</h1>
		<h2>Variable</h2>
		<p><%= tagline %></p>

		<ul>
			<% mascots.forEach( (mascot) => { %>
				<li>
					<strong><%= mascot.name %></strong>
					birth <%= mascot.birth_year %>
				</li>
			<% }); %>
		</ul>
        <p>Welcome to templating using EJS</p>
    </div>
</main>

<%- include('../includes/footer.ejs'); %>